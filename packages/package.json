{
  "name": "riski-react-ui",
  "version": "1.7.0",
  "description": "Riski react UI",
  "main": "dist/index.js",
  "module": "src/index.js",
  "author": "Denis Bykov",
  "license": "MIT",
  "dependencies": {
    "prop-types": "15.7.2"
  },
  "scripts": {
    "transpile": "babel src -d dist --ignore '**/*.spec.js,**/*.stories.js'",
    "jest": "jest --coverage --verbose --color"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "setupFiles": [
      "../../setupTests"
    ]
  }
}
